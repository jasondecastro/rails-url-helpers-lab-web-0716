<div>
  <% @students.each do |student| %>
    <div><%= link_to student.first_name + " " + student.last_name, student_path(student.id) %></div>
    <div><%= link_to "click here for students", students_path %></div>
    <div><%= link_to "activate student", activate_student_path(student.id) %></div>
  <% end %>
</div>